{
  "name": "vite-project",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "基于vite打包的vue3项目",
  "scripts": {
    "dev": "vite --open",
    "build-test": "vite build --mode test",
    "build-beta": "vite build --mode beta",
    "build": "vite build",
    "preview": "vite preview --mode preview",
    "prettier:check": "prettier --config .prettierrc.cjs --check \"src/**/*.{js,ts,css,vue}\"",
    "prettier:write": "prettier --config .prettierrc.cjs --check \"src/**/*.{js,ts,css,vue}\" --write",
    "eslint:check": "eslint --ext .js,.vue",
    "eslint:fix": "eslint --ext .js,.vue --fix",
    "lint": "eslint --ext .js,.ts,.vue",
    "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts",
    "format": "prettier .  --write",
    "prepare": "husky install"
  },
  "dependencies": {
    "axios": "^1.3.5",
    "element-plus": "^2.3.2",
    "lodash": "^4.17.21",
    "pinia": "^2.0.33",
    "qs": "^6.11.1",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@rollup/plugin-image": "^3.0.2",
    "@rollup/plugin-inject": "^5.0.3",
    "@vitejs/plugin-legacy": "^4.0.2",
    "@vitejs/plugin-vue": "^4.1.0",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.10.0",
    "husky": "^8.0.3",
    "less": "^4.1.3",
    "lint-staged": "^13.2.1",
    "prettier": "^2.8.7",
    "sass": "^1.60.0",
    "vite": "^4.2.0"
  },
  "lint-staged": {
    "*.js": "npm run lint"
  }
}